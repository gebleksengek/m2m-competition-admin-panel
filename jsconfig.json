{
    "compilerOptions": {
        "baseUrl": ".",
        "paths": {
            "@src/*": [
                "src/*"
            ],
            "@assets/*": [
                "src/@core/assets/*"
            ],
            "@components/*": [
                "src/@core/components/*"
            ],
            "@layouts/*": [
                "src/@core/layouts/*"
            ],
            "@store/*": [
                "src/redux/*"
            ],
            "@styles/*": [
                "src/@core/scss/*"
            ],
            "@configs/*": [
                "src/configs/*"
            ],
            "@utils": [
                "src/utility/Utils"
            ],
            "@hooks/*": [
                "src/utility/hooks/*"
            ]
        }
    }
}